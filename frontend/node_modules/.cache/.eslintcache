From = moment(document.getElementById(\"leave_from\").value).format(\n      \"YYYY-MM-DD\"\n    );\n\n    if (document.getElementById(\"pto_checkbox\").checked) {\n      var count = 0;\n      count = moment.duration(moment(dateTo).diff(moment(dateFrom))).asDays();\n      setLeaveInfo({ ...leaveInfo, use_pto_points: [count + 1] });\n    } else {\n      setLeaveInfo({ ...leaveInfo, use_pto_points: [0] });\n    }\n  }\n\n  return (\n    <>\n      {/* Buttons */}\n      <div className=\"m-2 flex flex-col\">\n        <button\n          className=\"my-2 btn btn-sm btn-active normal-case\"\n          onClick={() =>\n            document.getElementById(\"file_a_leave_btn\").showModal()\n          }\n        >\n          File a Leave\n        </button>\n\n        {/* Modal - File A Leave   */}\n        <dialog id=\"file_a_leave_btn\" className=\"modal\">\n          <div className=\"modal-box\">\n            <h3 className=\"font-bold text-lg\">File A Leave</h3>\n\n            <form\n              id=\"leaveForm\"\n              action=\"\"\n              method=\"dialog\"\n              onSubmit={handleSubmit}\n            >\n              <br />\n\n              {/* Dropdown - PTO Type */}\n\n              <label>\n                <div className=\"label\">\n                  <h1 className=\"label-text\">\n                    Type of Leave <span className=\"text-red-500\"> *</span>\n                  </h1>\n                </div>\n                <select\n                  name=\"leave_type\"\n                  className=\"select select-bordered w-full max-w-xs mb-2\"\n                  onChange={handleChange}\n                  required\n                >\n                  <option disabled selected>\n                    Pick a reason for filing a leave\n                  </option>\n                  <option>Sick Leave</option>\n                  <option>Bereavement Leave</option>\n                  <option>Maternity/Paternity Leave</option>\n                  <option>Vacation Leave</option>\n                  <option>Adverse Weather Leave</option>\n                  <option>Study Leave</option>\n                  <option>Emergency Leave</option>\n                  <option>Other/Special Leave</option>\n                </select>\n              </label>\n\n              <div className=\"flex\">\n                 {/* Date From */}\n                 <div className=\"flex-1 mx-1\">\n                 <label>\n                <div className=\"label\">\n                  <h1 className=\"label-text\">\n                    Date From <span className=\"text-red-500\"> *</span>\n                  </h1>\n                </div>\n                <input\n                  id=\"leave_from\"\n                  name=\"leave_from\"\n                  type=\"date\"\n                  placeholder=\"Type here\"\n                  className=\"input input-bordered w-full max-w-xs mb-2\"\n                  onChange={handleChange}\n                  onInput={disableNext}\n                  min={moment().format(\"YYYY-MM-DD\")}\n                  required\n                />\n              </label>\n                 </div>\n              \n                <div className=\"flex-1 mx-1\">\n                {/* Date To */}\n\n              <label className=\"form-control\">\n                <div className=\"label\">\n                  <h1 className=\"label-text\">\n                    Date To <span className=\"text-red-500\"> *</span>\n                  </h1>\n                </div>\n                <input\n                  id=\"leave_to\"\n                  name=\"leave_to\"\n                  type=\"date\"\n                  placeholder=\"Type here\"\n                  className=\"input input-bordered w-full max-w-xs mb-2\"\n                  min={moment().format(\"YYYY-MM-DD\")}\n                  onChange={handleChange}\n                  required\n                />\n              </label>\n                </div>\n              \n\n              </div>\n\n             \n\n              {/* Reason for Leave */}\n              <label className=\"form-control\">\n                <div className=\"label\">\n                  <h1 className=\"label-text\">\n                    Reason for Leave <span className=\"text-red-500\"> *</span>\n                  </h1>\n                </div>\n                <textarea\n                  id=\"leave_reason\"\n                  name=\"leave_reason\"\n                  className=\"textarea textarea-bordered w-full max-w-lg mb-2\"\n                  placeholder=\"Reason for Leave...\"\n                  onChange={handleChange}\n                  maxLength=\"255\"\n                  required\n                ></textarea>\n                <div className=\"label py-0\">\n                  <span className=\"label-text-alt\">\n                     \n                  </span>\n                  <span id=\"textarea-label\" className=\"label-text-alt\">\n                    0\n                  </span>\n                </div>\n              </label>\n\n              <label>\n                <div className=\"label\">\n                  <h1 className=\"label-text\">\n                    Approver <span className=\"text-red-500\"> *</span>\n                  </h1>\n                </div>\n                <select\n                  name=\"approver_id\"\n                  className=\"select select-bordered w-full max-w-xs mb-2\"\n                  onChange={handleChange}\n                  required\n                >\n                  <option disabled selected>\n                    Choose your approver\n                  </option>\n\n                  {approvers.map((appr) => (\n                    <option value={appr.emp_id}>\n                      {appr.f_name +\n                        \" \" +\n                        appr.s_name +\n                        \" (\" +\n                        appr.dept_name +\n                        \")\"}\n                    </option>\n                  ))}\n                </select>\n              </label>\n\n              {/* Current PTO Points */}\n              <h1 className=\"text-base\">Current PTO Points</h1>\n              {ptos.map((pto) => (\n                <h1 id=\"pto_points\" className=\"text-lg font-bold mb-2\">\n                  {pto.leave_balance}\n                </h1>\n              ))}\n\n              {/* Use PTO Checkbox */}\n              <div className=\"flex justify-start items-center\">\n                <input\n                  id=\"pto_checkbox\"\n                  name=\"use_pto_points\"\n                  type=\"checkbox\"\n                  className=\"checkbox checkbox-sm mr-3\"\n                  onChange={handleChange}\n                  //onClick={checkPTO}\n                />\n                <h1 id=\"pto_enough_label\" class=\"ptos_labels\">\n                  Use PTO credit(/s)?\n                </h1>\n              </div>\n\n              {/* Button Container */}\n              <div className=\"flex justify-end mt-3\">\n                <button\n                  onClick={usePTOpoints}\n                  type=\"submit\"\n                  className=\"btn btn-primary mr-2\"\n                >\n                  Submit\n                </button>\n\n                {/* Cancel Button */}\n                {/* If there is a button in form, it will close the modal */}\n                <button className=\"btn\" onClick={handleCancel}>\n                  Cancel\n                </button>\n              </div>\n            </form>\n          </div>\n        </dialog>\n      </div>\n    </>\n  );\n};\n\nexport default DashBButtons;\n","/Users/marvinbautista/Desktop/hris/frontend/src/components/universal/DashBAnniversaries.jsx",[],[],"/Users/marvinbautista/Desktop/hris/frontend/src/components/universal/DashBBirthdays.jsx",[],[],"/Users/marvinbautista/Desktop/hris/frontend/src/components/universal/DashBRemainingOffset.jsx",["273","274","275"],[],"import React, { useState, useEffect } from \"react\";\ncon